# Convert line endings in all text files
*            text=auto eol=lf


# Files with these endings are text files
*.bat        text eol=lf
*.css        text eol=lf
*.conf       text eol=lf
*.gradle     text eol=lf
*.graphml    text eol=lf
*.groovy     text eol=lf
*.html       text eol=lf
*.java       text eol=lf
*.js         text eol=lf
*.properties text eol=lf
*.sh         text eol=lf
*.sql        text eol=lf
*.svg        text eol=lf
*.scala      text eol=lf
*.template   text eol=lf
*.wsdl       text eol=lf
*.xjb        text eol=lf
*.xml        text eol=lf
*.xsd        text eol=lf

# Files with these endings are binary files
*.jar        -text
*.jpg        -text
*.png        -text
*.psd        -text
*.mp4        -text
*.swf        -text
